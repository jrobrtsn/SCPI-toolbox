#!@PYTHON@


import os
import sys
import signal
import locale
import gettext

VERSION = '@VERSION@'
pkgdatadir = '@pkgdatadir@'
localedir = '@localedir@'

sys.path.insert(1, pkgdatadir)
signal.signal(signal.SIGINT, signal.SIG_DFL)
locale.bindtextdomain('scpi_powersupply_control', localedir)
locale.textdomain('scpi_powersupply_control')
gettext.install('scpi_powersupply_control', localedir)

if __name__ == '__main__':
    import gi

    from gi.repository import Gio
    resource = Gio.Resource.load(os.path.join(pkgdatadir, 'scpi_powersupply_control.gresource'))
    resource._register()

    from scpi_powersupply_control import main
    sys.exit(main.main(VERSION))
